{
  "auth": {
    "header":     "X-Auth",
    "holder":     "secret required",
    "require": {
      "download": true,
      "list":     false,
      "upload":   false
    },
    "token":      "mySecret"
  },
  "audit": {
    "logFile":    "",
    "timeFormat": "Monday Jan 2 15:04"
  },
  "limits": {
    "downloads":  1,
    "duration":   "2m",
    "maxSizeMb":  20,
    "perMinute":  10,
    "ticker":     "30s"
  },
  "paths": {
    "download":   "/download",
    "heartbeat":  "/heartbeat",
    "list":       "/list",
    "static":     "/static",
    "upload":     "/upload"
  },
  "port":         8080
}
