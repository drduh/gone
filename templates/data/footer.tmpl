<!-- footer.tmpl -->
{{ define "footer" }}
    <div class="footer">
      <div class="message">{{ .Version.id }}
        <span class="owner">
          <a href="https://github.com/drduh/gone">
            {{ .Version.id }}-{{ .Version.vers }}</a>
          <br>built by {{ .Version.user }} on {{ .Version.arch }} {{ .Version.os }}
          <br>at {{ .Version.time }} using <a href="https://go.dev/">{{ .Version.go }}</a>
          <br>up {{ .Uptime}} on {{ .Hostname }}
        </span>
      </div>
      {{ if .Style.AllowPick }} {{ template "themeSelect" . }} {{ end }}
    </div>
  </body>
</html>
{{ end }}
